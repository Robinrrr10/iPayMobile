<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ipay.org.ipaymobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/myicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/myicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity_password" />
        <activity android:name=".Activity_Home" />
        <activity android:name=".My_Account" />
        <activity android:name=".Activity_Transfer" />
        <activity android:name=".Activity_Confirm_Transaction" />
        <activity android:name=".Activity_Recharge"></activity>
    </application>

</manifest>